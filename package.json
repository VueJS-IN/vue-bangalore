{
  "private": false,
  "name": "vue-bangalore",
  "version": "0.0.0",
  "description": "A Vue meetup in Bangalore, India.",
  "scripts": {
    "build": "vuepress build .",
    "serve": "vuepress dev .",
    "lint": "eslint \"./**/*.{js,vue}\"",
    "lint-fix": "yarn lint --fix",
    "prettier-fix": "prettier --write \".vuepress/**/*.{js,vue}\"",
    "prettier": "prettier --check \"./**/*.{js,vue}\"",
    "fix-formatting": "yarn run prettier-fix && yarn run lint-fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/znck/vue-bangalore.git"
  },
  "author": "Rahul Kadyan <hi@znck.me> (https://znck.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/znck/vue-bangalore/issues"
  },
  "homepage": "https://blr.vue.community",
  "dependencies": {
    "frontmatter": "^0.0.3",
    "globby": "^11.0.0",
    "vue-carousel": "^0.18.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "vuepress": "^1.2.0"
  }
}
